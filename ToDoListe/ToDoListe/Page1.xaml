<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="ToDoListe.Page1"
             Title="{Bidning nom}">
    <ContentPage.Content>
        <StackLayout Padding="20">

            <Label Text="{Binding nom}" FontSize="Body" FontAttributes="Bold" HorizontalTextAlignment="Center" />

            <Label Text="{Binding description}" FontSize="Body" FontAttributes="Bold"/>

        </StackLayout>
    </ContentPage.Content>
</ContentPage>